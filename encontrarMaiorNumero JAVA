package exercicios8;

public class Exercicios8 {

	public static void main(String[] args) {
		 int a = 10;
	     int b = 100;

	     int maiorNumero = encontrarMaiorNumero(a, b);

	    System.out.println("O maior nÃºmero Ã©: " + maiorNumero);
	    }

	    public static int encontrarMaiorNumero(int a, int b) {
	        if (a > b) {
	            return a;
	        } else {
	            return b;
	        }

	        
	}

}
